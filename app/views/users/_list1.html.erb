
 <h1>User info</h1>
 <%= image_tag user.get_profile_image(100,100) %>
 <table class="table">
   <tr>
     <td>name</td><td><%= user.name %></td>
   </tr>
   <tr>
     <td>introduction</td><td><%= user.introduction %></td>
   </tr>
 </table>
 <%= link_to edit_user_path(user), class: "btn btn-outline-secondary" do %>
 <i class="fas fa-user-cog"></i>
 <% end %>



