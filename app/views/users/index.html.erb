    <div class="container px-5 px-sm">
      <div class="row">
        <div class="col-md-3">
          <%= render 'list1', user: @user %>
          <%= render 'books/list', book: @book %>
        </div>
        <div class="col-md-8 offset-md-1">
          <h1>Users</h1>
          <table class="table table-hover table-inverse">
          <thead>
            <tr>
              <th scop="col">Image</th>
              <th scop="col">Name</th>
              <th scop="col"></th>
            </tr>
          </thead>
          <tbody>
          <% @users.each do |user| %>
            <tr>
              <td><%= image_tag user.get_profile_image(100,100) %></td>
              <td><%= user.name %></td>
              <td><%= link_to "Show", user_path(user.id) %></td>
            </tr>
          <% end %>
          </tbody>
          </table>
        </div>
      </div>
    </div>